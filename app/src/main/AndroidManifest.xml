<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oooof.jonnyjonny.whouseofprayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher28_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ContactActivity" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".InsideBoardActivity" android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MediaActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PrayerBoardActivity"
            android:parentActivityName=".EventActivity" />
        <activity
            android:name=".EventActivity"
            android:parentActivityName=".MissionActivity" />
        <activity
            android:name=".MissionActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>